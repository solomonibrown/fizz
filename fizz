include <stdio.h> 
 
int main(void) 
{ 
    int i; 
    for (i=1; i<=100; i++) 
    {  if ((i%3) == 0)     
            printf("Fizz\t");                             
        else // print the number             
            printf("%d\t", i);                  
   } 
  return 0 ; 
}
For ( var i=1; i <= 100; i++)
If ( i %3 ==0) {
console.log(“Fizz”);
}
Else {
console.log(i);
}
